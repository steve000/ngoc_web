//table表格里，地址列表默认和设置默认按钮显示样式
tr{
    position: relative;
    .showDef{
        position: absolute;
        border: 1px solid #ff9624;
        color: #ff9624;
        background: #ffeddd;
        padding: 3px 5px;
        top: 10px;
    }
    .showSetDef{
        position: absolute;
        display: none;
        background: #ff9624;
        color: #fff;
        border-radius: 1px;
        padding: 5px;
        top: 10px;
    }
    &:hover{
        .showSetDef{
            display: inline-block;
        }
    }
}
//列表页面内部布局 左边导航部分
.left-section{
    width: 160px;
    height: 100%;
    background-color: #E9EEF1;
    font-size: 16px;
    position: absolute;
    z-index: 100;
    box-sizing: border-box;
}
//列表页面内部布局 右边部分
.main-section{
    height: 100%;
    padding-left: 160px;
    background-origin: content-box;
    -webkit-background-origin: content-box;
    overflow: hidden;
    position: relative;
    background-color: #fff;
    .main-wrapper{
        width: 100%;
        height: 100%;
        padding: 0 20px;
    }
}
//新增、查看、编辑等详情页面 整体布局
.content{
    background: #fff;
    .title{
        width: 100%;
        height: 44px;
        line-height: 44px;
        padding-left: 20px;
        font-size: 14px;
        font-weight: bold;
        color: #333;
        background-color: #fff;
        border-bottom:1px solid #E5E5E5;
    }
    .search{
        width: 100%;
        padding:30px 20px 0 20px;
        background-color: #fff;
    }
    .content-wrapper{
        padding: 20px 20px 1px 20px;
        background-color: #fff;
        //新增里面的商品清单的可加减的table
        .native-table-show{
            width: 100%;
            border: 1px solid #eee;
            border-collapse: collapse;
            margin: 0 auto;
            text-align: left;
            font-size: 12px;
            .thead-tr-show{
                background: #F6F7FB;
                width: 100px;
                height: 44px;
            }
            .tbody-tr-show{
                border-bottom: 1px solid #eee;
                background: #FFFFFF;
                height: 44px;
                overflow: hidden;
                box-shadow: 0 1px 0 0 #F2F6FD;
                transition: 1s;
                .iconfont1{
                    width: 16px;
                    height: 16px;
                    top: -6px;
                    right: -1px;
                    position: absolute;
                    display: none;
                    background-color: white;
                    color: red;
                    transition: 1s;
                }
                &:hover{
                    outline: 1px dashed #539eff;
                    outline-offset: 0px;
                    transition: 1s;
                    .iconfont1{
                        display: block;
                        cursor: pointer;
                        transition: 1s;
                    }
                }
                td{
                    padding-right: 16px;

                    .table-operation{
                        position: relative;
                    }
                    .table-input{
                        width: 120px;
                    }
                }
            }
        }
    }
}
