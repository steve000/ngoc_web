        text-align: right;
        right: 20px;
    }
}
.text-link{
    color: #40b6fa;
    cursor: pointer;
    text-decoration-line: underline;
}
.el-table{
    border-left: none;
    thead tr th{
        font-size: 12px;
        background-color: #f6f7fb;
        color: #888;
        border: 1px solid #eee;
        border-top: none;
        border-right: none;
        padding: 0;
        .cell{
            line-height: 36px;
        }
    }
    tbody tr td{
        padding: 0;
        border-left: none;
        border-right: none;
        .cell{
            line-height: 44px;
            color: #666666;
            font-size: 12px;
        }
    }
    tbody tr{
        &:hover{
            td{
                background-color: #f6fafe !important;
            }
        }
