    text-align: right;
    padding: 20px;
}

.main-section {
    height: 100%;
    padding-left: 160px;
    background-origin: content-box;
    -webkit-background-origin: content-box;
    overflow: hidden;
    position: relative;
    background-color: #fff;
    .main-wrapper {
        width: 100%;
        height: 100%;
        padding: 0 20px;
    }
