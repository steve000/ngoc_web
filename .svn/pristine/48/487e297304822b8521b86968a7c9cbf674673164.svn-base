//新增、查看、编辑等详情页面 整体布局
.content {
    background: #fff;
    .title {
        width: 100%;
        height: 44px;
        line-height: 44px;
        padding-left: 20px;
        font-size: 14px;
        font-weight: bold;
        color: #333;
        background-color: #fff;
        border-bottom: 1px solid #E5E5E5;
    }
    .search {
        width: 100%;
        padding: 30px 20px 0 20px;
        background-color: #fff;
    }
    .content-wrapper {
        padding: 20px 20px 1px 20px;
        background-color: #fff;
        // form表单左对齐，非必填item label样式
        .reset-form-label {
            label {
                padding-left: 11px;
            }
        }
        //新增里面的商品清单的可加减的table
        .native-table-show {
            width: 100%;
            border: 1px solid #eee;
            border-collapse: collapse;
            margin: 0 auto;
            text-align: left;
            font-size: 12px;
            .thead-tr-show {
                background: #F6F7FB;
                width: 100px;
                height: 44px;
            }
            .tbody-tr-show {
                border-bottom: 1px solid #eee;
                background: #FFFFFF;
                height: 44px;
                overflow: hidden;
                box-shadow: 0 1px 0 0 #F2F6FD;
                transition: 1s;
                .iconfont1 {
                    width: 16px;
                    height: 16px;
                    top: -6px;
                    right: -16px;
                    position: absolute;
                    display: none;
                    background-color: white;
                    color: red;
                    transition: 1s;
                }
                &:hover {
                    outline: 1px dashed #539eff;
                    outline-offset: 0px;
                    transition: 1s;
                    .iconfont1 {
                        display: block;
                        cursor: pointer;
                        transition: 1s;
                    }
                }
                td {
                    padding-right: 16px;
                    .table-operation {
                        position: relative;
                    }
                    .table-input {
                        width: 120px;
                    }
                }
            }
        }
    }
}

