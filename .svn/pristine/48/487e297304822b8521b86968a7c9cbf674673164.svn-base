/**
NGOC 商品和仓库模块左边导航样式统一
 */

.menu-product {
    height: 100%;
    width: 100%;
    overflow-y: auto;
    padding-bottom: 75px;
    .menu-item-li {
        display: block;
        box-sizing: border-box;
        //width: 160px;
        color: rgb(66, 78, 103);
        height: auto;
        cursor: pointer;
        .menu-item-li {
            //opacity: 0.7;
            color: rgba(66, 78, 103, 0.7);
            height: auto;
            .set-border {
                padding-left: 68px;
                .positionIcon {
                    left: 48px;
                }
            }
            .menu-item-li {
                height: auto;
                .set-border {
                    padding-left: 78px;
                }
            }
        }
        .set-border {
            height: 36px;
            line-height: 36px;
            padding-left: 58px;
            position: relative;
            font-size: 12px;
            text-overflow: ellipsis;
            white-space: nowrap;
            overflow: hidden;
            &:hover {
                background-color: #DDE2E5;
            }
            .positionIcon {
                position: absolute;
                left: 38px;
                color: #B3BBCD;
                top: 14px;
                width: 0px;
                height: 0px;
                border-width: 0px 0px 8px 8px;
                border-style: solid;
                border-color: #B3BBCD transparent #B3BBCD transparent;
            }
        }
    }
    .isactive {
        color: #3B8CFF;
        background: #DDE2E5;
    }
    .all {
        cursor: pointer;
        color: #424E67;
        display: block;
        box-sizing: border-box;
        //width: 160px;
        .set-border {
            padding-left: 30px;
            height: 44px;
            font-size: 14px;
            line-height: 44px;
            &:hover {
                background-color: #DDE2E5;
            }
        }
        .icon-quanbu {
            font-size: 16px;
            margin-right: 12px;
        }
    }
    .submenu {
        display: none;
        height: auto;
    }
    .isopen {
        display: block;
    }
}

.menu {
    .menu-item {
        display: block;
        box-sizing: border-box;
    }
    .submenu {
        display: none;
    }
    .submenu .menu-item .set-border {
        padding-left: 40px;
    }
    .isopen {
        display: block;
    }
    .isactive .set-border {
        color: #3B8CFF;
        background-color: #dde2e5;
    }
    .menu-item:hover {
        background-color: #dde2e5;
    }
    .set-border {
        width: 100%;
        height: 100%;
        padding-left: 52px;
        display: inline-block;
        .iconquanbu {
            margin-left: 10px;
            margin-right: 5px;
        }
    }
    .all {
        font-size: 14px;
        padding-left: 20px;
    }
